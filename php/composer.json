{
    "name": "somlabs/intercoop",
    "version": "1.0.0",
    "description": "Intercooperation library",
    "keywords": ["business2business","socialeconomy"],
    "license": "MIT",
    "authors": [
        {
            "name": "David García Garzón",
            "email": "voki@canvoki.net"
        },
        {
            "name": "Martí Casas",
            "email": "mcasas@madsystems.coop"
        },
    {
            "name": "Roman Quintero",
            "email": "rquintero@madsystems.coop"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "phpseclib/phpseclib": "2.0.*",
        "ramsey/uuid": "*",
        "symfony/yaml": "*",
        "silex/silex": "~2.0",
        "silex/web-profiler": "~2.0",
        "symfony/asset": "~2.8|^3.0",
        "symfony/browser-kit": "~2.8|^3.0",
        "symfony/class-loader": "~2.8|^3.0",
        "symfony/config": "~2.8|^3.0",
        "symfony/console": "~2.8|^3.0",
        "symfony/css-selector": "~2.8|^3.0",
        "symfony/debug": "~2.8|^3.0",
        "symfony/finder": "~2.8|^3.0",
        "symfony/form": "~2.8|^3.0",
        "symfony/monolog-bridge": "~2.8|^3.0",
        "symfony/process": "~2.8|^3.0",
        "symfony/security": "~2.8|^3.0",
        "symfony/translation": "~2.8|^3.0",
        "symfony/twig-bridge": "~2.8|^3.0",
        "symfony/validator": "~2.8|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "SomLabs\\Intercoop\\": "src/",
            "SomLabs\\Intercoop\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ],
        "test": "phpunit --tap"
    }
}
